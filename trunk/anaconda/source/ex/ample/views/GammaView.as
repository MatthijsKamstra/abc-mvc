package ex.ample.views {	import assets.view.Gamma;	import mvc.core.View;	import com.greensock.TweenMax;	import flash.events.Event;	/**	 * @author Matthijs Kamstra aka [mck]	 */	public class GammaView extends View {		/**		 * Constructor		 */		public function GammaView( ) {			addChild(new Gamma());		}						/**		 * Method used to show this view.		 *		 * @param event		Event.		 */		override public function show( event : Event ) : void {			visible = true;			scaleX = scaleY = .5;			TweenMax.to(this, 1, {scaleX:1, scaleY:1});		}	}}