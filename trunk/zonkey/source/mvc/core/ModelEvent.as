package mvc.core {	import flash.events.Event;	/**	 * @author Stijn De Ryck	 */	public class ModelEvent extends Event {		public static const ALL_EVENTS : String = "onAllEvents";		public static const CLEAR : String = "_destroy";		private var _extraInfo:Object = undefined;		//private var _target : MovieClip;		public function ModelEvent(type : String, extraInfo:Object = null, bubbles : Boolean = false, cancelable : Boolean = false) { 			super(type, bubbles, cancelable);			this._extraInfo = extraInfo;		} 		public override function clone() : Event { 			return new  ModelEvent(type, _extraInfo, bubbles, cancelable);		} 		public override function toString() : String { 			return formatToString("ModelEvent", "extraInfo", "type", "bubbles", "cancelable", "eventPhase"); 		}				public function get extraInfo():Object { return _extraInfo; }				public function set extraInfo(value:Object):void 		{			_extraInfo = value;		}	}}