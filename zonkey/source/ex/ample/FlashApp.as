package ex.ample {	import ex.ample.views.AlfaView;	import ex.ample.views.BetaView;	import ex.ample.views.DeltaView;	import ex.ample.views.EpsilonView;	import ex.ample.views.GammaView;	import ex.ample.views.NavView;	import mvc.core.Application;	import mvc.core.Model;	/**	 * Dragon	 * ex.ample.FlashApp	 */	public class FlashApp extends Application {		/**		 * Constructor		 */		public function FlashApp(  ) {			trace("+ Dragon + FlashApp.FlashApp() - args: " + [  ]);		}		/**		 * Setup the application.		 */		override protected function init() : void {						model.data = new XML();						// shortest way of setting views			// first view initiated will be the default view (activated by Model.getInstance().showDefaultView())			addChild(new AlfaView());			addChild(new BetaView());			addChild(new GammaView());			addChild(new DeltaView());			addChild(new EpsilonView());			// place navigation on top, so add last			addChild(new NavView());						// show first/default view			Model.getInstance().showDefaultView();		}	}}